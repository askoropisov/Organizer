<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:Organizer"
             x:Class="Organizer.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <!--<StyleInclude Source="avares://Citrus.Avalonia/Sea.xaml"/>-->
        <FluentTheme Mode="Dark"/>
    </Application.Styles>

    <Application.Resources>
        <!--первые 2 символа это прозрачность-->
        <SolidColorBrush x:Key="SystemControlBackgroundAltHighBrush" Color="#FF2F4F4F"/>
    </Application.Resources>
</Application>
